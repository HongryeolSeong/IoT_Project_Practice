<Page x:Class="test.Final"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:test"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Final">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock x:Name="TxtThank" Text="감사합니다!" FontSize="40" Margin="100, 0, 100, 0"/>
            <TextBlock x:Name="TxtTable" HorizontalAlignment="Center" 
                       Text="선택하신 테이블은 ?입니다." Margin="0, 10, 0, 0"/>
            <TextBlock x:Name="TxtRes" HorizontalAlignment="Center" 
                       Text="예상 조리 시간은 00:00입니다." Margin="0, 10, 0, 10"/>
            <Button x:Name="BtnEnd" Width="50" Content="확인" Click="BtnEnd_Click" />
        </StackPanel>
    </Grid>
</Page>
